---
import Layout from '../layouts/Layout.astro'
import SavingsCalculatorPage from '../components/SavingsCalculator.astro'
---

<Layout
  title=" Savings Calculator – Estimate Your Potential Savings"
  description="Estimate healthcare savings with our calculator tool at NavixHealth. Optimize costs and manage expenses effectively with comprehensive insights."
  canonical="https://navixhealth.com/savings-calculator"
  openGraph={{
    url: 'https://navixhealth.com/savings-calculator',
    title: ' Savings Calculator – Estimate Your Potential Savings',
    description:
      'Estimate healthcare savings with our calculator tool at NavixHealth. Optimize costs and manage expenses effectively with comprehensive insights.',
  }}
>
  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "SoftwareApplication",
      "name": "Navix AI",
      "alternateName": "NavixScribe",
      "applicationCategory": "AI Powered Healthcare Software",
      "description": "NavixScribe is a HIPAA-compliant, AI-powered tool that transcribes your session or audio file and writes a formatted note. NavixScribe Groups is now available to transcribe the group session and write formatted notes for each participant. The future of behavioral health technology is here with NAVIX.",
      "url": "https://www.navixhealth.com/navixscribe",
      "creator": {
        "@type": "Organization",
        "name": "Navix Health",
        "url": "https://www.navixhealth.com"
      },
      "featureList": [
        "Automation for Efficiency",
        "Accuracy and Reliability",
        "Integration with Familiar Platforms",
        "Various Note Formats"
      ]
    }
  </script>

  <SavingsCalculatorPage />
</Layout>
