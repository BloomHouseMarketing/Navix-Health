---
import Layout from '../layouts/Layout.astro'
import FreeTrialPage from '../components/FreeTrial.astro'
---

<Layout
  title="Navix Health Free Trial - Manage Your Practice Efficiently"
  description="Start your free trial with NavixHealth to access comprehensive health insights and personalized wellness solutions tailored to your needs."
  canonical="https://navixhealth.com/start-free-trial"
  openGraph={{
    url: 'https://navixhealth.com/start-free-trial',
    title: 'Navix Health Free Trial - Manage Your Practice Efficiently',
    description:
      'Start your free trial with NavixHealth to access comprehensive health insights and personalized wellness solutions tailored to your needs.',
  }}
>
  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@type": "SoftwareApplication",
      "name": "Navix AI",
      "alternateName": "NavixScribe",
      "applicationCategory": "AI Powered Healthcare Software",
      "description": "NavixScribe is a HIPAA-compliant, AI-powered tool that transcribes your session or audio file and writes a formatted note. NavixScribe Groups is now available to transcribe the group session and write formatted notes for each participant. The future of behavioral health technology is here with NAVIX.",
      "url": "https://www.navixhealth.com/navixscribe",
      "creator": {
        "@type": "Organization",
        "name": "Navix Health",
        "url": "https://www.navixhealth.com"
      },
      "featureList": [
        "Automation for Efficiency",
        "Accuracy and Reliability",
        "Integration with Familiar Platforms",
        "Various Note Formats"
      ]
    }
  </script>

  <FreeTrialPage />
</Layout>
