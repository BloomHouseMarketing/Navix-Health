<main class='main-wrapper cc-visible'>
  <section class='section-home-hero'>
    <div class='padding-global padding-section-medium cc-home'>
      <div class='w-layout-blockcontainer container cc-home w-container'>
        <div class='home-hero-wrapper'>
          <div class='home-hero-block'>
            <h1
              data-aos-offset='40'
              data-aos='fade-up'
              data-aos-duration='1200'
              class='home-hero-heading'
